{
    "name": "foziluff/tools",
    "description": "A custom Swagger documentation generator for Laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "symfony/yaml": "^7.0",
        "laravel/framework": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Foziluff\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Foziluff\\ToolsServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
